    "react-icons": "^4.4.0",
    "react-select": "^5.4.0",
    "xstate": "^4.33.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@faker-js/faker": "^7.4.0",
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@vitest/coverage-c8": "^0.22.1",
    "autoprefixer": "^10.4.8",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "git-pull-run": "^1.0.2",
    "husky": "^8.0.1",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.2",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  },
  "wallaby": {
    "slowTestThreshold": 200,
    "runMode": "onSave",
    "hints": {
      "ignoreCoverageForFile": "/ignore file coverage/",
      "ignoreCoverage": "/ignore coverage/"
